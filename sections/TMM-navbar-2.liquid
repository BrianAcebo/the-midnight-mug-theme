{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  .tmm-section-{{ section.id }}.tmm-nav {
    margin-bottom: {{ section.settings.margin_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }

    .tmm-section-{{ section.id }}.tmm-nav {
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }

  .tmm-section-{{ section.id }} .tmm-button.solid,
  .tmm-section-{{ section.id }} .tmm-navbar1-menu_icon > div {
    background-color: {{ section.settings.button_color }};
    border: 1px solid {{ section.settings.button_color }};
  }

  .tmm-section-{{ section.id }} .tmm-button.outline {
    background-color: transparent;
    border: 1px solid {{ section.settings.button_color }};
    color: {{ section.settings.button_color }};
  }

  {%- if section.settings.show_line_separators -%}
    .tmm-section-{{ section.id }}.tmm-nav {
    	border-top: 1px solid #000;
    	border-bottom: 1px solid #000;
    }
  {%- endif -%}

  {%- if section.settings.sticky_header_type -%}
    .tmm-section-{{ section.id }}.tmm-nav {
      position: fixed;
      top: 0;
    }
  {%- endif -%}

  .tmm-section-{{ section.id }}.tmm-nav .tmm-nav-link,
  .tmm-section-{{ section.id }}.tmm-nav .tmm-dropdown-link,
  .tmm-section-{{ section.id }} .tmm-button {
    text-transform: {{ section.settings.text_transformation }};
    text-decoration: {{ section.settings.text_decoration }};
  }
{%- endstyle -%}

<div
  data-collapse="medium"
  data-animation="default"
  data-duration="400"
  fs-scrolldisable-element="smart-nav"
  data-easing="ease"
  data-easing2="ease"
  role="banner"
  class="tmm-navbar2-component tmm-nav"
>
  <div class="tmm-navbar2-container">
    <a
      href="#"
      class="tmm-navbar2-logo_link tmm-nav-brand"
      ><img
        loading="lazy"
        src="images/company-logo.svg"
        alt=""
        class="tmm-navbar2-logo"
    ></a>
    <nav
      role="navigation"
      class="tmm-navbar2-menu is-page-height-tablet tmm-nav-menu"
    >
      <a
        href="#"
        class="tmm-navbar2-link tmm-nav-link"
        >About Us</a
      >
      <a
        href="#"
        class="tmm-navbar2-link tmm-nav-link"
        >Our Story</a
      >
      <a
        href="#"
        class="tmm-navbar2-link tmm-nav-link"
        >Mission</a
      >
      <div
        data-delay="200"
        data-hover="true"
        class="tmm-navbar2-menu_dropdown tmm-dropdown"
      >
        <div class="tmm-navbar2-dropdown_toggle tmm-dropdown-toggle">
          <div>Services</div>
          <div class="tmm-dropdown-chevron tmm-embed">
            <svg
              width=" 100%"
              height=" 100%"
              viewbox="0 0 16 16"
              fill="none"
              xmlns="http://www.w3.org/2000/svg"
            >
              <path
              	fill-rule="evenodd"
              	clip-rule="evenodd"
              	d="M2.55806 6.29544C2.46043 6.19781 2.46043 6.03952 2.55806 5.94189L3.44195 5.058C3.53958 4.96037 3.69787 4.96037 3.7955 5.058L8.00001 9.26251L12.2045 5.058C12.3021 4.96037 12.4604 4.96037 12.5581 5.058L13.4419 5.94189C13.5396 6.03952 13.5396 6.19781 13.4419 6.29544L8.17678 11.5606C8.07915 11.6582 7.92086 11.6582 7.82323 11.5606L2.55806 6.29544Z"
              	fill="currentColor"
              ></path>
            </svg>
          </div>
        </div>
        <nav class="tmm-navbar2-dropdown_list tmm-dropdown-list">
          <a
            href="#"
            class="tmm-navbar2-dropdown_link tmm-dropdown-link"
            >Products</a
          >
          <a
            href="#"
            class="tmm-navbar2-dropdown_link tmm-dropdown-link"
            >Team</a
          >
          <a
            href="#"
            class="tmm-navbar2-dropdown_link tmm-dropdown-link"
            >Contact</a
          >
        </nav>
      </div>
      <div class="tmm-navbar2-menu_buttons">
        <a
          href="#"
          class="tmm-button tmm-is-secondary is-small tmm-button"
          >Learn</a
        >
        <a
          href="#"
          class="tmm-button is-small tmm-button"
          >Sign Up</a
        >
      </div>
    </nav>
    <div class="tmm-navbar2-menu_button tmm-nav-button">
      <div class="tmm-menu-icon1">
        <div class="tmm-menu-icon1_top"></div>
        <div class="tmm-menu-icon1_middle">
          <div class="tmm-menu-icon1_middle_inner"></div>
        </div>
        <div class="tmm-menu-icon1_bottom"></div>
      </div>
    </div>
  </div>
</div>

{% schema %}
{
  "name": "TMM - Navbar 2",
  "settings": [
    {
      "type": "color_scheme",
      "id": "color_scheme",
      "label": "Color Scheme",
      "default": "scheme-1"
    },
    {
      "type": "checkbox",
      "id": "sticky_header_type",
      "default": false,
      "label": "Sticky header"
    },
    {
      "type": "checkbox",
      "id": "show_line_separators",
      "default": true,
      "label": "Show line seperators"
    },
    {
      "type": "link_list",
      "id": "menu",
      "default": "main-menu",
      "label": "Menu"
    },
    {
      "type": "select",
      "id": "text_transformation",
      "options": [
        {
          "value": "none",
          "label": "None"
        },
        {
          "value": "uppercase",
          "label": "Uppercase"
        },
        {
          "value": "lowercase",
          "label": "Lowercase"
        },
        {
          "value": "capitalize",
          "label": "Capitalize"
        }
      ],
      "default": "capitalize",
      "label": "Text transform"
    },
    {
      "type": "select",
      "id": "text_decoration",
      "options": [
        {
          "value": "none",
          "label": "None"
        },
        {
          "value": "overline",
          "label": "Overline"
        },
        {
          "value": "underline",
          "label": "Underline"
        },
        {
          "value": "line-through",
          "label": "Line-through"
        },
        {
          "value": "underline overline",
          "label": "Overline-underline"
        }
      ],
      "default": "none",
      "label": "Text decoration"
    },
    {
      "type": "select",
      "id": "button_type",
      "options": [
        {
          "value": "solid",
          "label": "Solid"
        },
        {
          "value": "outline",
          "label": "Outline"
        }
      ],
      "default": "solid",
      "label": "Button style"
    },
    {
      "type": "text",
      "id": "button_text",
      "default": "Shop Now",
      "label": "Button text"
    },
    {
      "type": "url",
      "id": "button_link",
      "label": "Button link"
    },
    {
      "type": "color",
      "id": "button_color",
      "default": "#000000",
      "label": "Button color"
    },
    {
      "type": "header",
      "content": "Bottom spacing"
    },
    {
      "type": "range",
      "id": "margin_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "Margin bottom",
      "default": 0
    },
    {
      "type": "header",
      "content": "Section padding"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 36,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_top",
      "default": 20
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 36,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 20
    }
  ],
   "presets": [
    {
      "name": "TMM - Navbar 2"
    }
  ]
}
{% endschema %}
